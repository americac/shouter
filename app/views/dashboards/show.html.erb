<!-- Since the dashboard controller is only create one object
this form needs to be modified because the objects expected
are no longer being passed on by the controller.
ie @text_should, @photo_shout and shouts variable
-->
<%= form_for @dashboard.new_text_shout do |form| %>
  <%= form.text_field :body, placeholder: 'Shout content here' %>
  <%= form.submit 'Shout!' %>
<% end %>

<%= form_for @dashboard.new_photo_shout do |form| %>
  <%= form.file_field :image %>
  <%= form.submit 'Shout!' %>
<% end %>

<%= render @dashboard.timeline %>
